@{ Layout = "_Layout";}


<link rel="stylesheet" type="text/css" href="~/assets/contact_formvendor/bootstrap/css/bootstrap.min.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formfonts/font-awesome-4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formfonts/iconic/css/material-design-iconic-font.min.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formvendor/animate/animate.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formvendor/css-hamburgers/hamburgers.min.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formvendor/animsition/css/animsition.min.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formvendor/select2/select2.min.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formvendor/daterangepicker/daterangepicker.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_formcss/util.css">
<link rel="stylesheet" type="text/css" href="~/assets/contact_form/css/main.css">


<div class="container-contact100">
    <div class="wrap-contact100">
        <form class="contact100-form" action="ClientEnquiry" method="post" id="fid" style="color:red!important;">
            <span class="contact100-form-title">
                Contact Us
            </span>

            <label class="label-input100" for="first-name" style="color:black!important;">Your Name <span style="color:red!important;">*</span></label>
            <div class="wrap-input100 rs1 validate-input">
                <input id="first-name" class="input100" type="text" name="firstname" placeholder="First name" required>
                <span class="focus-input100"></span>
            </div>
            <div class="wrap-input100 rs1 validate-input">
                <input class="input100" type="text" name="lastname" placeholder="Last name" >
                <span class="focus-input100"></span>
            </div>

            <label class="label-input100" for="email" style="color:black!important;">Email Address <span style="color:red!important">*</span></label>
            <div class="wrap-input100 validate-input">
                <input id="email" class="input100" type="email" name="email" placeholder="Eg. example@email.com" required>
                <span class="focus-input100"></span>
            </div>

            <label class="label-input100" for="phone" style="color:black!important;">Phone Number</label>
            <div class="wrap-input100">
                <input id="phone" class="input100" type="text" name="phone" placeholder="Eg. +263 800 000000" >
                <span class="focus-input100"></span>
            </div>

            <label class="label-input100" for="message" style="color:black!important;">Message <span style="color:red!important">*</span></label>
            <div class="wrap-input100 validate-input">
                <textarea id="message" class="input100" name="message" placeholder="Please enter your comments..." required></textarea>
                <span class="focus-input100"></span>
            </div>

            <div class="container-contact100-form-btn">
                <button class="contact100-form-btn" type="submit">
                    <span>
                        Submit
                        <i class="zmdi zmdi-arrow-right m-l-8"></i>
                    </span>
                </button>
            </div>
        </form>
    </div>
</div>




<script src="~/assets/contact_form/vendor/jquery/jquery-3.2.1.min.js"></script>
<script src="~/assets/contact_form/vendor/animsition/js/animsition.min.js"></script>
<script src="~/assets/contact_form/vendor/bootstrap/js/popper.js"></script>
<script src="~/assets/contact_form/vendor/bootstrap/js/bootstrap.min.js"></script>
<script src="~/assets/contact_form/vendor/select2/select2.min.js"></script>
<script src="~/assets/contact_form/vendor/daterangepicker/moment.min.js"></script>
<script src="~/assets/contact_form/vendor/daterangepicker/daterangepicker.js"></script>
<script src="~/assets/contact_form/vendor/countdowntime/countdowntime.js"></script>
<script src="~/assets/contact_form/js/main.js."></script>

<script src="~/assets/libs/jquery_validator/jquery.validate.js"></script>

<script>
    $("#fid").validate();
    $("#fid").on("submit", function (e) {
        var valid = $("#fid").valid();
        if (!valid) e.preventDefault();
     });
   
</script>